        "concurrently": "^6.1.0",
        "dotenv": "^9.0.2",
        "eslint": "^7.26.0",
        "sass": "^1.32.13",
        "typescript": "^4.1.3",
        "vite": "^2.1.5",
        "@typescript-eslint/eslint-plugin": "^4.15.2",
        "@typescript-eslint/parser": "^4.15.2"
